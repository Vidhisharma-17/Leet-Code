class Solution {
public:
    vector<int> rightSideView(TreeNode* root) {
        vector<int> temp;
        queue<TreeNode*> que;
        que.push(root);
        if(root==NULL){
            return {};
        }
        while(!que.empty()){
            int n=que.size();
            TreeNode* node=NULL;
        while(n--){
            node=que.front();
            que.pop();
            if(node->left!=NULL){
                que.push(node->left);
            }
            if(node->right!=NULL){
                que.push(node->right);
            }
        }
            temp.push_back(node->val);
        }    
        return temp;
    }
};
