class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<pair<int,int>> twosum;
        int n=nums.size();
        for(int i=0;i<n;i++){
            twosum.push_back({nums[i],i});
        }
        sort(twosum.begin(),twosum.end());
        int low=0;
        int high=twosum.size()-1;
        while(low<high){
            int sum = twosum[low].first + twosum[high].first;
            if(sum==target){
                return {twosum[low].second, twosum[high].second};
               
            }
            else if(sum<target){
                low++;
            }
            else{
                high--;
            }
       
        }    
       return {};

        
    }
};
