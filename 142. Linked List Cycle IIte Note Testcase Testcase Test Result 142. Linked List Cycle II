class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        ListNode *slow=head;
        ListNode *entry=head;
        ListNode *fast=head;
        if(head==NULL){
            return NULL;
        }
      
        while(fast->next!=NULL && fast->next->next!= NULL){
            slow=slow->next;
           
            fast=fast->next->next;
            if(slow==fast){
                while(entry!=slow){
                    slow=slow->next;
                    entry=entry->next;
                }
                return slow;
            }
            
            
        }
        return NULL;
        
        
    }
};
